<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>caraOuCoroaBean</managed-bean-name>
  <managed-bean-class>com.ibm.bean.CaraOuCoroaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/pagina1.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>proximo</from-outcome>
   <to-view-id>/pagina2.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/componentes-visuais.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>proximo</from-outcome>
   <to-view-id>/templates-e-modularizacao.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/templates-e-modularizacao.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>proximo</from-outcome>
   <to-view-id>/navegacao.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>anterior</from-outcome>
   <to-view-id>/componentes-visuais.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/navegacao.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>anterior</from-outcome>
   <to-view-id>/templates-e-modularizacao.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <display-name>cara-ou-coroa.xhtml</display-name>
  <from-view-id>/cara-ou-coroa.xhtml</from-view-id>
  <navigation-case>
   <from-action>#{caraOuCoroaBean.proxima}</from-action>
   <from-outcome>coroa</from-outcome>
   <to-view-id>/coroa.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-action>#{caraOuCoroaBean.proxima}</from-action>
   <from-outcome>cara</from-outcome>
   <to-view-id>/cara.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/cara.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cara-ou-coroa</from-outcome>
   <to-view-id>/cara-ou-coroa.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/coroa.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cara-ou-coroa</from-outcome>
   <to-view-id>/cara-ou-coroa.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
